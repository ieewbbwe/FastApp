<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true"><shape android:padding="5dip" android:shape="rectangle">
            <solid android:color="#88d9d9d9" />

            <corners android:radius="4dip" />
        </shape></item>
    <item android:state_pressed="false"><shape android:padding="5dip" android:shape="rectangle">
            <solid android:color="#FFd9d9d9" />

            <corners android:radius="4dip" />
        </shape></item>
    <item><shape android:padding="5dip" android:shape="rectangle">
            <solid android:color="#FFd9d9d9" />

            <corners android:radius="4dip" />
        </shape></item>

</selector>
